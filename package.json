{
  "name": "@mtucourses/frontend",
  "version": "0.1.0",
  "description": "",
  "repository": "git@github.com:Michigan-Tech-Courses/frontend.git",
  "author": "Max Isom <hi@maxisom.me>",
  "license": "MIT",
  "scripts": {
    "lint": "xo",
    "lint:fix": "xo --fix",
    "build": "next build",
    "build:export": "next build && next export",
    "build:analyze": "ANALYZE=true yarn build",
    "build:profile": "PROFILE=true yarn build",
    "find:unused": "next-unused",
    "start": "next start",
    "dev": "next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "@next/bundle-analyzer": "^10.0.7",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/dom-to-image": "^2.6.3",
    "@types/jest": "^26.0.14",
    "@types/lunr": "^2.3.4",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "@types/react-big-calendar": "^0.33.1",
    "@types/react-dom": "^17.0.9",
    "@types/workbox-window": "^4.3.4",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint-config-xo": "0.43.1",
    "eslint-config-xo-react": "0.27.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^10.5.4",
    "next-pwa": "^5.4.6",
    "next-unused": "^0.0.6",
    "nodemon": "^2.0.7",
    "sass": "^1.32.8",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "type-fest": "^1.0.2",
    "typescript": "^4.9.5",
    "xo": "^0.56.0"
  },
  "next-unused": {
    "include": [
      "components"
    ]
  },
  "xo": {
    "plugins": [
      "mobx"
    ],
    "rules": {
      "import/extensions": "off",
      "unicorn/no-array-reduce": "off",
      "node/file-extension-in-import": "off",
      "react/no-unescaped-entities": "off",
      "react/prop-types": "off",
      "react/jsx-no-useless-fragment": "off",
      "node/prefer-global/process": "off",
      "unicorn/prefer-module": "off",
      "node/prefer-global/buffer": "off",
      "import/no-cycle": "off",
      "@typescript-eslint/naming-convention": "off",
      "n/prefer-global/process": "off",
      "n/prefer-global/buffer": "off",
      "unicorn/no-await-expression-member": "off",
      "@typescript-eslint/ban-types": "off",
      "mobx/missing-observer": "error"
    },
    "envs": [
      "browser",
      "node",
      "commonjs"
    ],
    "ignores": [
      "next-env.d.ts",
      "next.config.js"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "xo --fix"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/test/*.integration.ts*",
      "**/*.test.ts",
      "**/*.test.tsx"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "<rootDir>/test/mocks.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "\\.tsx?$": "ts-jest",
      "\\.jsx?$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.json"
      }
    }
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.9",
    "@chakra-ui/system": "^1.12.1",
    "@chakra-ui/theme-tools": "^1.3.6",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@nivo/core": "^0.73.0",
    "@nivo/line": "^0.73.0",
    "@nivo/tooltip": "^0.73.0",
    "@rschedule/core": "^1.2.3",
    "@rschedule/json-tools": "^1.2.3",
    "@rschedule/standard-date-adapter": "^1.2.3",
    "@veccu/react-calendar": "^2.3.2",
    "bowser": "^2.11.0",
    "chrome-aws-lambda": "^8.0.2",
    "date-fns": "^2.23.0",
    "date-fns-tz": "^1.1.6",
    "dom-to-image": "^2.6.0",
    "eslint-plugin-mobx": "0.0.9",
    "framer-motion": "^4.1.17",
    "ical-generator": "^3.0.1",
    "lunr": "^2.3.9",
    "mapbox-gl": "^2.4.1",
    "memoize-one": "^5.2.1",
    "mobx": "^6.10.2",
    "mobx-persist-store": "^1.1.3",
    "mobx-react-lite": "^4.0.5",
    "mobx-shallow-undo": "^1.1.0",
    "nanoid": "^3.1.29",
    "next": "^12.1.0",
    "next-absolute-url": "^1.2.2",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^4.26.0",
    "p-throttle": "^4.1.1",
    "process": "0.11.10",
    "prop-types": "^15.7.2",
    "puppeteer-core": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-hotkeys-hook": "^3.4.0",
    "react-mapbox-gl": "^5.1.1",
    "react-reverse-portal": "^2.1.1",
    "react-use": "^17.3.1",
    "workbox-window": "7.0.0"
  }
}
